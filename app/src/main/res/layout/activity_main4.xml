        android:text="TOTAL TIME WHEN LEFT AND ARRIVED"
        android:textColor="#B07CD9"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
